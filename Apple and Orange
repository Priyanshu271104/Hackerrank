#include <bits/stdc++.h>
using namespace std;
int main(){
    int s,t,i,a,b,m,n,count_a=0,count_o=0;
    cin>>s>>t;
    cin>>a>>b;
    cin>>m>>n;
    int apple[m],orange[n];
        for (i=0;i<m;i++)
        {cin>>apple[i];}
        for(i=0;i<n;i++)
        {cin>>orange[i];}
        for (i=0;i<m;i++)
        {if((a+apple[i]>=s)&&(a+apple[i]<=t))
        count_a+=1;}
        for (i=0;i<n;i++)
        {if((b+orange[i]>=s)&&(b+orange[i]<=t)) 
        count_o+=1;}
       cout<<count_a<<endl;
        cout<<count_o;
    return 0;
}
