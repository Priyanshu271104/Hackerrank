#include <math.h>
#include <stdio.h>


int main(){
    int a[2],b[2];
    int a_score = 0;
    int b_score = 0,i;
    for(i=0;i<=2;i++)
    {scanf("%d",&a[i]);}
    for(i=0;i<=2;i++){
    scanf("%d",&b[i]);}
    for(i=0;i<=2;i++)
    {if (a[i] > b[i])
    a_score+=1;
    else if(a[i]<b[i])
    b_score+=1;
    else if(a[i]==b[i])
    {a_score+=0;
    b_score+=0;}
    else {
    continue;
    }    }

    printf("%d %d",a_score,b_score);
 return 0;
}
