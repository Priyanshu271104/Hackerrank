#include<stdio.h>
#include<math.h>
int main(){
    int t;
    scanf("%d",&t);
    int a[t],i;
    float p=0,n=0,z=0;
    for(i=0;i<t;i++)
    scanf("%d",&a[i]);
    for(i=0;i<t;i++)
    {if(a[i]>0)
    p+=1;
    else if(a[i]<0)
    {n+=1;}
    else
    z+=1;}
    printf("%f\n%f\n%f\n",p/t,n/t,z/t);
    return 0;
}
