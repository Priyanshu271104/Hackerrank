#include <bits/stdc++.h>
using namespace std;
int main(){
    char t[10];
    int i;
    for(i=0;i<10;i++)
    {
        cin>>t[i];}
    if(t[8]=='P')
    {
        if(t[0]=='1'&&t[1]=='2')
        {
            t[0]='1';    
            t[1]='2';
        }
        else
        {t[0]+=1;    
        t[1]+=2;}}
        
    else if(t[8]=='A') 
    {
        if(t[0]=='1'&&t[1]=='2')
        {
            t[0]='0';
            t[1]='0';
        }

}
for(i=0;i<8;i++)
{
    cout<<t[i];}
return 0;

}
