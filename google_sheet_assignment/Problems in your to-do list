#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,i,count=0;cin>>n;
    vector<int> a(n);
    for(i=0;i<n;i++){
        cin>>a[i];
        count=0;
        vector<int> b(a[i]);
        for(int j=0;j<a[i];j++){
            cin>>b[j];
        }
        for(int j=0;j<a[i];j++){
            if(b[j]>=1000)
            count+=1;}
    cout<<count<<endl;
    }
    return 0;
}
